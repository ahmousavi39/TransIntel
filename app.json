{
  "expo": {
    "name": "TransIntel",
    "slug": "transintel",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#b93ec7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.transintel.app",
      "infoPlist": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Images",
            "LSHandlerRank": "Alternate",
            "LSItemContentTypes": ["public.image", "public.png", "public.jpeg"]
          },
          {
            "CFBundleTypeName": "PDFs",
            "LSHandlerRank": "Alternate", 
            "LSItemContentTypes": ["com.adobe.pdf"]
          },
          {
            "CFBundleTypeName": "Audio",
            "LSHandlerRank": "Alternate",
            "LSItemContentTypes": ["public.audio", "public.mp3"]
          },
          {
            "CFBundleTypeName": "Text",
            "LSHandlerRank": "Alternate",
            "LSItemContentTypes": ["public.text", "public.plain-text"]
          }
        ],
        "UISupportsDocumentBrowser": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#b93ec7"
      },
      "package": "com.transintel.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
